#include <stdio.h>

int main() {
    int n, i, sum = 0, actualSum, missing;

   
    printf("Enter the size of the array (n): ");
    scanf("%d", &n);

    int arr[n-1]; 
    printf("Enter %d elements (numbers from 0 to %d with one missing):\n", n-1, n);

    // Input elements
    for(i = 0; i < n-1; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i]; 
    }

    actualSum = n * (n + 1) / 2;

   
    missing = actualSum - sum;

    printf("The missing number is: %d\n", missing);

    return 0;
}
